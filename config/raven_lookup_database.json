  "usda_soil_texture_classification": {
    "description": "USDA soil texture triangle classification rules for dynamic soil class assignment",
    "classification_rules": {
      "CLAY": {
        "conditions": "clay >= 40",
        "subconditions": {
          "SANDY_CLAY": "sand >= 45",
          "SILTY_CLAY": "silt >= 40",
          "CLAY": "default"
        }
      },
      "CLAY_LOAM_GROUP": {
        "conditions": "clay >= 27 and clay < 40",
        "subconditions": {
          "SANDY_CLAY_LOAM": "sand >= 45",
          "SILTY_CLAY_LOAM": "silt >= 28", 
          "CLAY_LOAM": "default"
        }
      },
      "LOAM_GROUP": {
        "conditions": "clay >= 7 and clay < 27",
        "subconditions": {
          "SANDY_LOAM": "sand >= 52",
          "SILT_LOAM": "silt >= 50",
          "LOAM": "default"
        }
      },
      "SAND_GROUP": {
        "conditions": "clay < 7",
        "subconditions": {
          "SILT": "silt >= 80",
          "SAND": "sand >= 85",
          "LOAMY_SAND": "sand >= 70",
          "LOAM": "default"
        }
      }
    }
  },

