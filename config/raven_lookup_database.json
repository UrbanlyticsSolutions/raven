      "manning_roughness_generator": {
        "function": "generate_manning_roughness_csv",
        "output_format": "RasterV,MannV",
        "mapping_source": "manning_roughness_database.roughness_values.landcover_based",
        "output_file": "manning_roughness.csv"
      }
    },
    "raven_parameter_extraction": {
      "soil_parameters": {
        "function": "extract_soil_parameters",
        "source": "soil_classification.soil_classes",
        "output_section": ":SoilClasses",
        "attributes": ["POROSITY", "FIELD_CAPACITY", "WILTING_POINT", "SAT_WILT", "HBV_BETA", "HBV_LP", "SAT_HYDRAULIC_CONDUCTIVITY"],
        "units": ["none", "mm", "mm", "mm", "none", "none", "mm/hr"]
      },
      "vegetation_parameters": {
        "function": "extract_vegetation_parameters", 
        "source": "landcover_classification.landcover_classes",
        "output_section": ":VegetationClasses",
        "attributes": ["MAX_HEIGHT", "MAX_LAI", "MAX_LEAF_CONDUCTANCE", "ALBEDO"],
        "units": ["m", "none", "mm_per_s", "none"]
      },
      "landuse_parameters": {
        "function": "extract_landuse_parameters",
        "source": "landcover_classification.landcover_classes", 
        "output_section": ":LandUseClasses",
        "attributes": ["IMPERMEABLE_FRAC", "FOREST_COV", "MANNING_N", "INTERCEPTION_CAPACITY"],
        "units": ["none", "none", "none", "mm"]
      }
    },
    "hru_class_mapping": {
      "soil_texture_classifier": {
        "function": "classify_soil_texture_from_percentages",
        "method": "usda_texture_triangle",
        "input": ["sand_percent", "silt_percent", "clay_percent"],
        "output": "soil_class_name"
      },
      "landcover_classifier": {
        "function": "classify_landcover_from_raster",
        "method": "worldcover_to_raven_mapping",
        "input": "worldcover_class_id",
        "output": "raven_landcover_class"
      },
      "hru_parameter_assigner": {
        "function": "assign_hru_parameters",
        "inputs": ["hru_gdf", "soil_raster", "landcover_raster", "dem_raster"],
        "outputs": ["soil_class", "landcover_class", "elevation_stats", "slope", "aspect"]
      }
    },
    "seasonal_variation": {
      "monthly_lai_generator": {
        "function": "generate_monthly_lai",
        "source": "landcover_classification.landcover_classes.seasonal_variation",
        "output_format": "monthly_lai_timeseries.csv"
      },
      "monthly_canopy_generator": {
        "function": "generate_monthly_canopy_cover",
        "source": "landcover_classification.landcover_classes.seasonal_variation", 
        "output_format": "monthly_canopy_timeseries.csv"
      }
    },
    "validation": {
      "parameter_range_validator": {
        "function": "validate_parameter_ranges",
        "soil_ranges": {
          "porosity": [0.2, 0.7],
          "field_capacity": [50.0, 500.0], 
          "hydraulic_conductivity": [1.0, 200.0]
        },
        "vegetation_ranges": {
          "max_lai": [0.0, 10.0],
          "max_height": [0.1, 50.0],
          "albedo": [0.05, 0.35]
        }
      },
      "consistency_checker": {
        "function": "check_parameter_consistency",
        "checks": ["soil_classes_exist", "landcover_classes_exist", "parameter_completeness"]
      }
    }
  },

  "worldcover_to_raven_mapping": {
    "description": "Direct mapping from ESA WorldCover classes to RAVEN landcover classes",
    "mappings": {
      "10": {"raven_class": "FOREST_CONIFEROUS", "raven_id": 10},
      "20": {"raven_class": "SHRUBLAND", "raven_id": 95},
      "30": {"raven_class": "GRASSLAND", "raven_id": 60},
      "40": {"raven_class": "AGRICULTURE", "raven_id": 40},
      "50": {"raven_class": "URBAN", "raven_id": 50},
      "60": {"raven_class": "BARREN", "raven_id": 70},
      "70": {"raven_class": "SNOW_ICE", "raven_id": 71},
      "80": {"raven_class": "WATER", "raven_id": 80},
      "90": {"raven_class": "WETLAND", "raven_id": 90},
